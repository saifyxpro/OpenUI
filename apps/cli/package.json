{
  "name": "openui",
  "module": "src/index.ts",
  "type": "module",
  "version": "0.0.1",
  "description": "OpenUI CLI - IDE Agent Bridge & Development Toolbar",
  "license": "MIT",
  "private": false,
  "bin": {
    "openui": "dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "dotenv -e ../../.env -- tsx build.ts",
    "dev": "NODE_ENV=development dotenv -e ../../.env -- tsx src/index.ts",
    "run": "dotenv -e ../../.env -- rsx dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest run --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@inquirer/prompts": "^7.0.0",
    "@openui-xio-plugins/angular": "workspace:*",
    "@openui-xio-plugins/react": "workspace:*",
    "@openui-xio-plugins/vue": "workspace:*",
    "@openui-xio/karton": "workspace:*",
    "@openui-xio/toolbar-bridged": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "dotenv-cli": "^9.0.0",
    "env-paths": "^3.0.0",
    "esbuild": "0.25.9",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "ignore": "^6.0.2",
    "open": "^10.0.0",
    "typescript": "^5",
    "vitest": "^2.0.0",
    "winston": "^3.17.0",
    "zod": "^3.24.0"
  }
}
